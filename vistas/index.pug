html
    head
        title= titulo
        link(rel="stylesheet", href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200")
        link(rel="stylesheet", href="styles.css")
        link(rel="preconnect" href="https://fonts.googleapis.com")
        link(rel="preconnect", href="https://fonts.gstatic.com")
        link(href="https://fonts.googleapis.com/css2?family=Cabin&family=Dela+Gothic+One&family=Nova+Flat&display=swap" rel="stylesheet")
    body 
        div.container
            input(type="text" placeholder="nombre" id="nombre")
            input(type="text" placeholder="numero" id="numero")
            button.btn(id="btn_agregar") agregar contacto
            div.agenda= agenda
                
                each contacto in contactos 
                    article.card
                        p= `${contacto.id_agenda} // ${contacto.nombre_contacto} // ${contacto.numero_contacto}`
                        span(class="borrar material-symbols-outlined" id=contacto.id_agenda) delete
            
    script(src="funciones.js")